{
  "$id": "Duration.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "title": "Duration",
  "properties": {
    "t90": {
      "type": "number",
      "description": "Time duration of central 90% of integrated fluence [s]"
    },
    "t90_error": {
      "type": "number",
      "description": "1-sigma uncertainty of the T90 duration [s]"
    },
    "t50": {
      "type": "number",
      "description": "Time duration of central 50% of integrated fluence [s]"
    },
    "t50_error": {
      "type": "number",
      "description": "1-sigma uncertainty of the T50 duration [s]"
    }
  }
}
