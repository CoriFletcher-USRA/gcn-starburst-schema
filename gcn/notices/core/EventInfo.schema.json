{
  "$id": "https://gcn.nasa.gov/schema/gcn/notices/core/EventInfo.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "title": "EventInfo",
  "description": "Name or names of the event",
  "properties": {
    "event_id": {
      "type": "string",
      "description": "Name of the event"
    },
    "event_alias": {
      "type": "array",
      "description": "0 or more alias event identfiers"
    }
  },
  "required": ["event_id", "event_alias"]
}
